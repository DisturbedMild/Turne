{% set mobile = '' %}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Title</title>
    <link href="https://fonts.googleapis.com/css?family=Montserrat:500,600,700|Open+Sans:400,600,700&display=swap&subset=cyrillic"
          rel="stylesheet">
    <link href="css/bootstrap/bootstrap.css?v=113" rel="stylesheet"/>
    <link href="css/bootstrap/bootstrap-grid.css?v=113" rel="stylesheet"/>
    <link rel="stylesheet" href="css/ui-lightness/jquery-ui-1.9.2.custom.min.css">
    <link rel="stylesheet" href="css/ui-lightness/jquery-ui.structure.min.css">
    <link rel="stylesheet" href="css/bundle/global.css?v=113">

    {% block searchStyle %}{% endblock %}

    {% block head %}{% endblock %}

    <link href="css/bundle/oldGlobal.css?v=113" rel="stylesheet"/>


</head>
<body class="{{ mobile }}">
<div class="wrap-content">
    {% include "../components/header/header.twig" %}
    <div class="main-content">
        {% block content %}{% endblock %}
    </div>
    {% include "../components/footer/footer.twig" %}
</div>
{% include "../components/modal/sign-in-modal.twig" %}
{% include "../components/modal/callback-header-modal.twig" %}
{% block modals %}{% endblock %}
<script src="js/jquery-3.3.1.min.js"></script>
<script src="js/popper.min.js" type="text/javascript"></script>
<script src="js/bootstrap.min.js" type="text/javascript"></script>
<script src="js/jquery-ui.js" type="text/javascript"></script>
<script src="js/datepicker.js" type="text/javascript"></script>
<script src="js/svg4everybody.min.js" type="text/javascript"></script>
<script src="js/ofi.min.js" type="text/javascript"></script>
<script src="js/scripts.js" type="text/javascript"></script>
<script src="js/scripts-desktop.js" type="text/javascript"></script>
<script src="js/lazysizes.min.js" async=""></script>

{% block scripts %}{% endblock %}
</body>
</html>